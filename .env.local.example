# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Inngest Configuration
# Get these values from https://app.inngest.com
# Create a free account and get your event key and signing key
INNGEST_EVENT_KEY=your_inngest_event_key_here
INNGEST_SIGNING_KEY=your_inngest_signing_key_here

# Google Drive Import Timeouts (optional)
# Increase these if imports are timing out on large folders/files
# GOOGLE_DRIVE_DEFAULT_TIMEOUT_MS=300000  # 5 minutes (default)
# GOOGLE_DRIVE_LIST_TIMEOUT_MS=300000     # 5 minutes for folder listings (default)
# GOOGLE_DRIVE_EXPORT_TIMEOUT_MS=180000   # 3 minutes for file exports (default)