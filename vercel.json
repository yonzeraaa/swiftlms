{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "swiftedu.com.br"
        }
      ],
      "destination": "https://www.swiftedu.com.br/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/docs/:path*",
      "destination": "https://docs.google.com/:path*"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  }
}